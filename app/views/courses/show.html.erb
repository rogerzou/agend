<% if !current_user.instructor %>
  <h1 class="welcome">My Office Hours</h1>
<% else %>
  <h1 class="welcome"><%= @thisCourse.course_title_long%></h1>
<% end %>

<div class="profcontainer-in">
  <% if !current_user.instructor %>
    <div>
      <div class="center-side">
      </div>
    </div>
    <br><br>
  <% else %>
    <div>
      <%= link_to "+ Default Session", new_office_hour_path(
              thisCourse: @thisCourse.id, default: true), class: "btn" %>
      <%= link_to "+ Update Session", new_office_hour_path(
              thisCourse: @thisCourse.id, default: false), class: "btn" %>
    </div>
  <% end %>
  <div>
    <%= render @myHours, thisCourse: @thisCourse.id %>
  </div>
</div>